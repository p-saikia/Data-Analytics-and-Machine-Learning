library(kernlab)
library(caret)
library(e1071)
rm (list=ls())
cat("\014") 
corrosion <- read.csv("corrosion_data.csv")
str(corrosion)
summary(corrosion)
corrosion$Label <- as.factor(corrosion$Label)
corrosion.scale <- data.frame(scale(corrosion[, 1:7], center = TRUE, scale = TRUE))
corrosion.scale$Label <- corrosion$Label
train_sample <- createDataPartition(y=corrosion.scale$Label, p=0.75, list = FALSE) 
corrosion_train <- corrosion.scale[train_sample,] 
corrosion_test <- corrosion.scale[-train_sample,]
corrosion_model_vanilladot <- ksvm(Label ~ ., data = corrosion_train, kernel = "vanilladot")
corrosion_pred_vanilladot <- predict(corrosion_model_vanilladot, corrosion_test)
confusionMatrix(corrosion_pred_vanilladot, corrosion_test$Label, positive = "1")
corrosion_model_rbfdot <- ksvm(Label ~ ., data = corrosion_train, kernel = "rbfdot")
corrosion_pred_rbfdot <- predict(corrosion_model_rbfdot, corrosion_test)
confusionMatrix(corrosion_pred_rbfdot, corrosion_test$Label, positive = "1")
corrosion_model_polydot <- ksvm(Label ~ ., data = corrosion_train, kernel = "polydot")
corrosion_pred_polydot <- predict(corrosion_model_polydot, corrosion_test)
confusionMatrix(corrosion_pred_polydot, corrosion_test$Label, positive = "1")
corrosion_model_tanhdot <- ksvm(Label ~ ., data = corrosion_train, kernel = "tanhdot")
corrosion_pred_tanhdot <- predict(corrosion_model_tanhdot, corrosion_test)
confusionMatrix(corrosion_pred_tanhdot, corrosion_test$Label, positive = "1")
corrosion_model_laplacedot <- ksvm(Label ~ ., data = corrosion_train, kernel = "laplacedot")
corrosion_pred_laplacedot <- predict(corrosion_model_laplacedot, corrosion_test)
confusionMatrix(corrosion_pred_laplacedot, corrosion_test$Label, positive = "1")
corrosion_model_besseldot <- ksvm(Label ~ ., data = corrosion_train, kernel = "besseldot")
corrosion_pred_besseldot <- predict(corrosion_model_besseldot, corrosion_test)
confusionMatrix(corrosion_pred_besseldot, corrosion_test$Label, positive = "1")
corrosion_model_anovadot <- ksvm(Label ~ ., data = corrosion_train, kernel = "anovadot")
corrosion_pred_anovadot <- predict(corrosion_model_anovadot, corrosion_test)
confusionMatrix(corrosion_pred_anovadot, corrosion_test$Label, positive = "1")
corrosion_model_splinedot <- ksvm(Label ~ ., data = corrosion_train, kernel = "splinedot")
corrosion_pred_splinedot <- predict(corrosion_model_splinedot, corrosion_test)
confusionMatrix(corrosion_pred_splinedot, corrosion_test$Label, positive = "1")
train_sample <- createDataPartition(y=corrosion.scale$Label, p=0.75, list = FALSE) 
corrosion_train <- corrosion.scale[train_sample,] 
corrosion_test <- corrosion.scale[-train_sample,]
corrosion_model_rbfdot <- ksvm(Label ~ ., data = corrosion_train, kernel = "rbfdot")
corrosion_pred_rbfdot <- predict(corrosion_model_rbfdot, corrosion_test)
confusionMatrix(corrosion_pred_rbfdot, corrosion_test$Label, positive = "1")
corrosion_model_laplacedot <- ksvm(Label ~ ., data = corrosion_train, kernel = "laplacedot")
corrosion_pred_laplacedot <- predict(corrosion_model_laplacedot, corrosion_test)
confusionMatrix(corrosion_pred_laplacedot, corrosion_test$Label, positive = "1")
corrosion_model_anovadot <- ksvm(Label ~ ., data = corrosion_train, kernel = "anovadot")
corrosion_pred_anovadot <- predict(corrosion_model_anovadot, corrosion_test)
confusionMatrix(corrosion_pred_anovadot, corrosion_test$Label, positive = "1")
train_sample <- createDataPartition(y=corrosion.scale$Label, p=0.75, list = FALSE) 
corrosion_train <- corrosion.scale[train_sample,] 
corrosion_test <- corrosion.scale[-train_sample,]
corrosion_model_rbfdot <- ksvm(Label ~ ., data = corrosion_train, kernel = "rbfdot")
corrosion_pred_rbfdot <- predict(corrosion_model_rbfdot, corrosion_test)
confusionMatrix(corrosion_pred_rbfdot, corrosion_test$Label, positive = "1")
corrosion_model_laplacedot <- ksvm(Label ~ ., data = corrosion_train, kernel = "laplacedot")
corrosion_pred_laplacedot <- predict(corrosion_model_laplacedot, corrosion_test)
confusionMatrix(corrosion_pred_laplacedot, corrosion_test$Label, positive = "1")
corrosion_model_anovadot <- ksvm(Label ~ ., data = corrosion_train, kernel = "anovadot")
corrosion_pred_anovadot <- predict(corrosion_model_anovadot, corrosion_test)
confusionMatrix(corrosion_pred_anovadot, corrosion_test$Label, positive = "1")
